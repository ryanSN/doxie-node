{
  "name": "doxie-node",
  "version": "1.2.0",
  "description": "A Node.js API wrapper for Doxie go Wi-fi, Doxie Q, and Doxie Go SE",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "lib"
  ],
  "repository": "ryanSN/doxie-node.git",
  "bugs": "https://github.com/ryanSN/doxie-node/issues",
  "author": "ryanSN <chatters@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "clean": "rm -rf dist",
    "build": "yarn run build:es && yarn run build:cjs",
    "build:es": "NODE_ENV=es ./node_modules/.bin/rollup -c",
    "build:cjs": "NODE_ENV=cjs ./node_modules/.bin/rollup -c",
    "prepublish": "yarn clean && yarn build"
  },
  "keywords": [
    "node",
    "doxie",
    "doxie q",
    "doxie go se",
    "doxie go",
    "doxie go wi-fi",
    "api",
    "wrapper",
    "js"
  ],
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "husky": "^2.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.5",
    "nock": "^10.0.1",
    "prettier": "^1.14.3",
    "rollup": "^1.10.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-node-resolve": "^5.0.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "retry": "^0.12.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8"
  }
}
